    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    }
